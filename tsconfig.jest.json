{
    "compilerOptions": {
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "esModuleInterop": true,
    "module": "commonjs",
    "allowSyntheticDefaultImports": true,
    "isolatedModules": false,
    "noEmit": true,
    "target": "ES6",
    "lib": ["ES6", "DOM"],
    "moduleResolution": "node",
  },
    "exclude": [
        "node_modules"
    ]
}
